{
  "type": "project",
  "description": "Simple PHP project for the DevSecOps class",
  "require": {
    "php": ">=8.1.0",
    "ext-gd": "*",
    "nesbot/carbon": "^2.72.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^10.3",
    "sebastian/diff": "5.0.3",
    "vimeo/psalm": "^5.0",
    "phpmd/phpmd": "^2.13",
    "niels-de-blaauw/php-doc-check": "^0.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
